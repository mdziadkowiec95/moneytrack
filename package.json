{
  "name": "moneytrack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "migrate:dev": "dotenv -e .env.local prisma migrate dev",
    "migrate:roll-back": "prisma migrate resolve --rolled-back",
    "prisma": "prisma",
    "start": "next start",
    "start:dev": "next start -p 3000",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}' --quiet",
    "lint-fix": "eslint '*/**/*.{js,jsx,ts,tsx}' --fix",
    "lint-staged": "lint-staged",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "test": "dotenv -e .env.test prisma migrate deploy && dotenv -e .env.test vitest",
    "test-ci": "dotenv -e .env.test prisma migrate deploy && dotenv -e .env.test vitest run",
    "test-e2e": "dotenv -e .env.local prisma migrate dev && playwright test --ui",
    "test-e2e-ci": "playwright test",
    "prepare": "husky",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.2.0",
    "@prisma/client": "^5.9.1",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/themes": "^2.0.3",
    "@types/d3": "^7.4.3",
    "@types/jsonwebtoken": "^9.0.5",
    "chart.js": "^4.4.1",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.4",
    "jsonwebtoken": "^9.0.2",
    "next": "14.1.0",
    "next-auth": "^4.24.5",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-tailwindcss-datepicker": "^1.6.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "postcss-import": "^16.0.1",
    "prettier": "^3.2.5",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "vitest": "^1.2.2",
    "vitest-mock-extended": "^1.3.1"
  }
}
